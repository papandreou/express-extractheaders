{
  "name": "express-extractheaders",
  "version": "1.2.0",
  "description": "Express middleware for setting response headers based on <meta http-equiv=... content=...> tags in the response body",
  "main": "lib/expressExtractHeaders.js",
  "scripts": {
    "test": "mocha",
    "prepublish": "npm test",
    "lint": "jshint .",
    "travis": "npm run lint && npm run coverage && cat ./coverage/lcov.info | coveralls",
    "coverage": "NODE_ENV=development istanbul cover _mocha -- --reporter dot && echo google-chrome coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/papandreou/express-extractheaders.git"
  },
  "keywords": [
    "express",
    "middleware",
    "headers",
    "meta",
    "http-equiv"
  ],
  "dependencies": {
    "hijackresponse": "1.0.1",
    "htmlparser2-papandreou": "3.9.1-patch1"
  },
  "devDependencies": {
    "coveralls": "2.11.2",
    "express": "4.10.1",
    "istanbul": "^0.3.2",
    "jshint": "2.5.2",
    "mocha": "3.0.2",
    "serve-static": "1.7.1",
    "sinon": "1.17.5",
    "unexpected": "10.15.1",
    "unexpected-express": "8.3.0",
    "unexpected-sinon": "10.2.1"
  },
  "publishConfig": {
    "repository": "http://registry.npmjs.org/"
  },
  "author": "Andreas Lind <andreas@one.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/papandreou/express-extractheaders/issues"
  },
  "homepage": "https://github.com/papandreou/express-extractheaders"
}
