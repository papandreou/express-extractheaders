{
  "name": "express-extractheaders",
  "version": "2.1.2",
  "description": "Express middleware for setting response headers based on <meta http-equiv=... content=...> tags in the response body",
  "main": "lib/expressExtractHeaders.js",
  "scripts": {
    "test": "mocha",
    "prepublish": "npm test",
    "lint": "eslint .",
    "ci": "npm run lint && npm run coverage",
    "coverage": "NODE_ENV=development nyc --reporter=lcov --reporter=text --all -- npm test && echo google-chrome coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/papandreou/express-extractheaders.git"
  },
  "keywords": [
    "express",
    "middleware",
    "headers",
    "meta",
    "http-equiv"
  ],
  "dependencies": {
    "hijackresponse": "3.0.0",
    "htmlparser2": "3.10.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "eslint": "^5.12.0",
    "eslint-config-pretty-standard": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "express": "^4.14.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "serve-static": "^1.11.1",
    "sinon": "^7.0.0",
    "unexpected": "^11.0.0",
    "unexpected-express": "^11.0.0",
    "unexpected-sinon": "^10.5.0"
  },
  "author": "Andreas Lind <andreaslindpetersen@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/papandreou/express-extractheaders/issues"
  },
  "homepage": "https://github.com/papandreou/express-extractheaders",
  "nyc": {
    "include": [
      "lib/**"
    ]
  }
}
